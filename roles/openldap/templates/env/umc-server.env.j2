DOMAINNAME={{ openldap_domain_name }}
HOSTNAME=default

WEB_SIZELIMIT=2000
LOCALE_DEFAULT=de_DE.UTF-8:UTF-8
DISPATCH_INTERVAL=100
MAX_CONNECTIONS=100
PROCESSES=1
SSL_CIPHERS=DEFAULT

PRIVATE_KEY_FILE=/run/secrets/private_key
CA_CERT_FILE=/run/secrets/ca_cert
CERT_PEM_FILE=/run/secrets/cert_pem

LDAP_URI=ldap://{{ openldap_host }}:{{ openldap_ldap_port }}
LDAP_BASE={{ openldap_base_dn }}
LDAP_MACHINE_PASSWORD={{ openldap_password }}
LDAP_ADMIN_PASSWORD={{ openldap_password }}
TLS_REQCERT=never
